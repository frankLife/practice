<table>
  <tr>
    <td>id</td>
    <td>hours</td>
    <td>date</td>
    <td>archived</td>
    <td>description</td>
  </tr>
<% for(var i = 0,len = rows.length;i<len;i++) { %>
  <tr>
    <td><%=rows[i]['id']%></td>
    <td><%=rows[i]['hours']%></td>
    <td><%=rows[i]['date']%></td>
    <td><%=rows[i]['archived']%></td>
    <td><%=rows[i]['description']%></td>
    <td><button class="J_del">delete</button><% if(rows[i]['archived'] == 0) {%><button class="J_archived">archive</button><%} %></td>
  </tr>
<% } %>
</table> 